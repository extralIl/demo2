<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.extra.demo.mapper.CourseMapper">
    <select id="com.extra.demo.mapper.CourseMapper.selectCourseListByCourseIdList" resultType="com.extra.demo.bean.Course">
        select *
        from course
        where course_id = #{courseId}
    </select>
</mapper>